@model OnlineOtomasyon.Entities.Concrete.Kategori
@{
    ViewBag.Title = "Ekle";
}

<form action="/Kategori/Ekle" method="post">
    <label for="kategoriAd" class="form-label mt-4">Kategori Adı</label>
    @Html.TextBoxFor(c => c.KategoriAdi, new { @class = "form-control", @id = "kategoriAd" })
    <button type="submit" class="btn btn-primary mt-3">Ekle</button>
</form>

